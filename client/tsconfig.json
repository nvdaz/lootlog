{
  "compilerOptions": {
    "outDir": "./dist/",
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "lib": [
      "DOM",
      "ES2020"
    ],
    "sourceMap": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "jsxFactory": "h",
    "allowJs": true,
    "typeRoots": ["types"]
  }
}